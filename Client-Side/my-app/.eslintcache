   addIngredientHandler = (type) => {\r\n        const oldCount = this.state.ingredients[type];\r\n        const updatedCounted = oldCount + 1;\r\n        const updatedIngredients = {\r\n            ...this.state.ingredients\r\n        };\r\n\r\n        updatedIngredients[type] = updatedCounted;\r\n        const priceAddition = INGREDIENT_PRICES[type];\r\n        const oldPrice = this.state.totalPrice;\r\n        const newPrice = oldPrice + priceAddition;\r\n\r\n        this.setState({ totalPrice: newPrice, ingredients: updatedIngredients });\r\n    }\r\n\r\n    removeIngredientHandler = (type) => {\r\n        if(this.state.ingredients[type] > 0) {\r\n            const oldCount = this.state.ingredients[type];\r\n            const updatedCounted = oldCount - 1;\r\n            const updatedIngredients = {\r\n                ...this.state.ingredients\r\n            };\r\n    \r\n            updatedIngredients[type] = updatedCounted;\r\n            const priceDeduction = INGREDIENT_PRICES[type];\r\n            const oldPrice = this.state.totalPrice;\r\n            const newPrice = oldPrice - priceDeduction;\r\n    \r\n            this.setState({ totalPrice: newPrice, ingredients: updatedIngredients });\r\n        } else {\r\n            console.log('No ingredients',this.state.ingredients[type]);\r\n        }\r\n    }\r\n        \r\n\r\n    render() {\r\n        const disbaledInfo = {\r\n            ...this.state.ingredients\r\n        };\r\n\r\n        for (const key in disbaledInfo) {\r\n            disbaledInfo[key] = disbaledInfo[key] <= 0;\r\n        }\r\n\r\n        return (\r\n            <Auxiliary>\r\n                <Recipe ingredients={this.state.ingredients}/>\r\n                <BuildControls \r\n                    ingredientAdded={this.addIngredientHandler} \r\n                    ingredientRemoved={this.removeIngredientHandler}\r\n                    disabled={disbaledInfo}\r\n                    totalPrice={this.state.totalPrice}\r\n                />\r\n            </Auxiliary>\r\n        );\r\n    }\r\n}\r\n\r\nexport default RecipeBuilder;","C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\index.js",[],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\features\\counter\\counterSlice.js",[],["107","108"],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\serviceWorker.js",[],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\containers\\WorkoutBuilder\\WorkoutBuilder.js",["109"],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\components\\Recipe\\RecipeIngredient\\RecipeIngredient.js",[],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\components\\Recipe\\Recipe.js",["110","111"],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\components\\Recipe\\BuildControls\\BuildControl\\BuildControl.js",["112","113"],"import React, { Component } from 'react';\r\nimport PropTypes from 'prop-types';\r\nimport classes from './BuildControl.module.css';\r\n\r\n  \r\nconst buildControl = (props) => (\r\n    <div className={classes.BuildControl}>\r\n        <div className={classes.Label}>{props.label}</div>\r\n        <button \r\n            className={classes.Less} \r\n            onClick={props.removed} \r\n            disabled={props.disabled}>Less</button>\r\n        <button \r\n            className={classes.More} \r\n            onClick={props.added}>More</button>\r\n    </div>\r\n);\r\n\r\nexport default buildControl;","C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\components\\Recipe\\BuildControls\\BuildControls.js",["114","115"],"import React, { Component } from 'react';