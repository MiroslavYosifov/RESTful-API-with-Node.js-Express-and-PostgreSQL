t updatedCounted = oldCount - 1;\r\n            const updatedIngredients = {\r\n                ...this.state.ingredients\r\n            };\r\n    \r\n            updatedIngredients[type] = updatedCounted;\r\n            const priceDeduction = INGREDIENT_PRICES[type];\r\n            const oldPrice = this.state.totalPrice;\r\n            const newPrice = oldPrice - priceDeduction;\r\n    \r\n            this.setState({ totalPrice: newPrice, ingredients: updatedIngredients });\r\n        } else {\r\n            console.log('No ingredients',this.state.ingredients[type]);\r\n        }\r\n    }\r\n        \r\n\r\n    render() {\r\n        const disbaledInfo = {\r\n            ...this.state.ingredients\r\n        };\r\n\r\n        for (const key in disbaledInfo) {\r\n            disbaledInfo[key] = disbaledInfo[key] <= 0;\r\n        }\r\n\r\n        return (\r\n            <Auxiliary>\r\n                <Recipe ingredients={this.state.ingredients}/>\r\n                <BuildControls \r\n                    ingredientAdded={this.addIngredientHandler} \r\n                    ingredientRemoved={this.removeIngredientHandler}\r\n                    disabled={disbaledInfo}\r\n                    totalPrice={this.state.totalPrice}\r\n                />\r\n            </Auxiliary>\r\n        );\r\n    }\r\n}\r\n\r\nexport default RecipeBuilder;","C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\index.js",[],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\features\\counter\\counterSlice.js",[],["96","97"],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\serviceWorker.js",[],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL\\Client-Side\\my-app\\src\\containers\\WorkoutBuilder\\WorkoutBuilder.js",["98"],"C:\\Users\\Miro\\Desktop\\api\\RESTful-API-with-Node.js-Express-and-PostgreSQL